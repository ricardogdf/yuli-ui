{
  "extends": "./tsconfig.json",
  "compilerOptions": {
    "tsBuildInfoFile": "./node_modules/.tmp/tsconfig.app.tsbuildinfo",
    "target": "ES2020",
    "module": "ESNext",
    "lib": ["DOM", "ES2020"],
    "jsx": "react-jsx",
    "moduleResolution": "Node",
    "noEmit": false,
    "skipLibCheck": true,
    "strict": true,
    "isolatedModules": true
  },
  "include": ["src"]
}
